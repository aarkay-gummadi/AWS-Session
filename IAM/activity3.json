{
    "Version": "2012-10-17"
    "Statement": [
        {
            "Action": ["s3:*", "rds:*", "ec2:*"],
            "Resource": ["*"],
            "Effect": "Allow"
        },
        {
            "Action": ["ec2:TerminateInstances", "rds:DeleteDBInstances", "s3:DeleteBucket"],
            "Resource": ["*"],
            "Effect": "Deny"
        }
    ]
}    